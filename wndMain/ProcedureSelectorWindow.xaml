<Window x:Class="HydroSystemModelPreProcess.ProcedureSelectorWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:HydroSystemModelPreProcess"
        xmlns:sys="clr-namespace:System;assembly=mscorlib"
        mc:Ignorable="d"
        Title="ProcedureSelectorWindow" Height="300" Width="300">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition></ColumnDefinition>
            <ColumnDefinition></ColumnDefinition>
        </Grid.ColumnDefinitions>

        <Grid.RowDefinitions>
            <RowDefinition Height="*"></RowDefinition>
            <RowDefinition Height="auto"></RowDefinition>
        </Grid.RowDefinitions>

        <ListBox Name="lbxProcedureSelector" Grid.ColumnSpan="2" Grid.Row="0" Margin="2">
            <ListBox.Items>
                <sys:String>恒定流水头损失计算</sys:String>
            </ListBox.Items>
        </ListBox>

        <Button Name="btnConfirm" Grid.Column="0" Grid.Row="1" Margin="2" Click="OnButtonConfirmClick">确定</Button>

        <Button Name="btnCancel" Grid.Column="1" Grid.Row="1" Margin="2" Click="OnButtonCancelClick">取消</Button>
    </Grid>
</Window>
